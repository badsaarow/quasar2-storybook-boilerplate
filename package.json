{
  "name": "quasar2-storybook-boilerplate",
  "version": "1.0.2",
  "description": "quasar2-storybook-boilerplate",
  "productName": "Quasar2 Storybook Boilerplate",
  "author": "sungyong<sungyong@gmail.com>",
  "private": true,
  "scripts": {
    "prepare": "husky install && yarn run patch",
    "patch": "cp -f ./patch/NormalModule.js.patch ./node_modules/webpack/lib/NormalModule.js",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "lint:fix": "yarn run lint --fix",
    "dev": "quasar dev",
    "dev:storybook": "start-storybook -p 6007",
    "dev:browser": "quasar dev",
    "dev:android": "quasar dev -m cordova -T android",
    "dev:ios": "quasar dev -m cordova -T ios",
    "build": "quasar build -d",
    "build:storybook": "build-storybook",
    "build:browser": "quasar build -m cordova -T browser",
    "build:android": "quasar build -m cordova -T android",
    "build:ios": "quasar build -m cordova -T ios",
    "test:unit": "jest --updateSnapshot",
    "test:unit:ci": "jest --ci",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "test:e2e": "quasar dev & cypress open",
    "test:e2e:ci": "cypress run",
    "serve:test:lighthouse": "quasar serve test/lighthouse/reports/ --port 8789",
    "audit:snyk": "snyk test --json > ./test/audits/snyk.json",
    "audit:node_modules": "yarn audit --json > ./test/audits/node_modules_audit.json",
    "audit:licenses": "nlf --summary detail > ./test/audits/licenses.txt",
    "audit:lighthouse": "lighthouse http://localhost:3000 view --output-path=./test/lighthouse/reports/index.html --config-path=./test/lighthouse/configs/full-config.js",
    "chromatic": "npx chromatic",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\""
  },
  "dependencies": {
    "@quasar/extras": "^1.15.1",
    "@vue-composable/axios": "^1.0.0-dev.0",
    "@vue/composition-api": "^1.6.1",
    "@vueuse/core": "^8.8.0",
    "axios": "^0.27.2",
    "core-js": "^3.22.7",
    "crypto-js": "^4.1.1",
    "lottie-web": "^5.9.4",
    "quasar": "^2.7.0",
    "secure-ls": "^1.2.6",
    "swiper": "^8.2.3",
    "vue": "^3.2.34",
    "vue-composable": "^1.0.0-dev.0",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.1.1",
    "vuex": "^4.0.2",
    "vuex-persistedstate": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/preset-typescript": "^7.13.0",
    "@cypress/vue": "^4.0.0",
    "@cypress/webpack-dev-server": "^2.0.0",
    "@logux/eslint-config": "^47.2.0",
    "@quasar/app": "^3.3.3",
    "@quasar/quasar-app-extension-testing": "^2.0.3",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^4.1.1",
    "@quasar/quasar-app-extension-testing-unit-jest": "^3.0.0-alpha.10",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/vue3": "^6.4.19",
    "@types/jest": "^28.1.4",
    "@types/node": "18.0.1",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vue/cli-service": "^5.0.4",
    "@vue/compiler-sfc": "^3.2.33",
    "@vue/eslint-config-typescript": "^11.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.2.5",
    "babel-preset-vue": "^2.0.2",
    "chromatic": "^6.5.4",
    "clean-publish": "^4.0.0",
    "css-loader": "^6.7.1",
    "cypress": "^10.3.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-let": "^3.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-storybook": "^0.5.11",
    "eslint-plugin-unicorn": "^43.0.0",
    "eslint-plugin-vue": "^9.1.1",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "husky": "^8.0.1",
    "jest": "^28.0.2",
    "jest-transform-stub": "^2.0.0",
    "lighthouse": "^9.5.0",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.5.3",
    "miragejs": "^0.1.41",
    "nlf": "^2.1.1",
    "postcss": "^8.4.12",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-focus": "^5.0.1",
    "postcss-import": "^14.1.0",
    "postcss-preset-env": "^7.4.4",
    "postcss-sharec-config": "^0.5.2",
    "prettier": "^2.6.2",
    "simple-git-hooks": "^2.7.0",
    "snyk": "^1.563.0",
    "storybook-addon-playwright": "^4.12.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.7.0",
    "url-loader": "^4.1.1",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-loader": "^17.0.0",
    "vue3-jest": "^27.0.0-alpha.1",
    "wallaby-vue-compiler": "^1.0.3"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">=14.0",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "peerDependencies": {
    "@vue/eslint-config-typescript": "^7.0.0",
    "postcss": "^8.2.14"
  },
  "lint-staged": {
    "*.{ts,vue,js,css,md}": [
      "yarn run prettier",
      "yarn run lint:fix --cache"
    ],
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "resolutions": {
    "vue-jest": "^5.0.0-alpha.10"
  },
  "eslintConfig": {
    "extends": "@logux/eslint-config"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "prettier": {
    "arrowParens": "avoid",
    "jsxSingleQuote": false,
    "quoteProps": "consistent",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "clean-publish": {
    "cleanDocs": true
  },
  "sharec": {
    "config": "postcss-sharec-config",
    "version": "0.5.2"
  }
}
